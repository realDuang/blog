{
  "version": "2.0",
  "envId": "{{env.envId}}",
  "framework": {
    "name": "Static-Website-Deploy",
    "plugins": {
      "client": {
        "use": "@cloudbase/framework-plugin-website",
        "inputs": {
          "installCommand": "npm ci",
          "buildCommand": "npm run build",
          "outputPath": "public",
          "cloudPath": "/"
        }
      }
    }
  }
}
